{
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://atcoder.jp/contests/*"]
    }
  ],
  "description": "Add a contest schedule to your Calendar",
  "manifest_version": 2,
  "name": "AtCoder Calendar",
  "short_name": "AC Calender",
  "version": "1.1.0",
  "permissions": ["activeTab", "https://atcoder.jp/*"],
  "icons": {
    "16": "images/16icon.png",
    "48": "images/48icon.png",
    "128": "images/128icon.png"
  }
}
